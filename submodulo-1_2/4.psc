Algoritmo cuatro
	Escribir "Dime el primer numero: ";
	Leer num1;
	Escribir "Dime el segundo numero: ";
	Leer num2;
	Si num1==num2 Entonces
		Escribir "Multiplicando: ", num1 " x ", num2, " = ", num1 * num2;
	FinSi
	Si num1>num2 Entonces
		Escribir "Restando: ", num1 " - ", num2, " = ", num1 - num2;
	SiNo
		Escribir "Sumando: ", num1 " + ", num2, " = ", num1 + num2;
	FinSi
FinAlgoritmo
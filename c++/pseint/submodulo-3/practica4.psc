Algoritmo practica4
	Escribir 'Dame el valor 1: '
	Leer num1
	Escribir 'Dame el valor 2: '
	Leer num2
	Si num1==num2 Entonces
		Escribir 'Multiplicando: ',num1*num2
	FinSi
	Si num1>num2 Entonces
		Escribir 'Restando: ',num1-num2
	SiNo
		Escribir 'Sumando: ',num1+num2
	FinSi
FinAlgoritmo
